{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [ "last 1 version", "ie >= 11" ]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-regenerator",
    "react-hot-loader/babel"
  ]
}
